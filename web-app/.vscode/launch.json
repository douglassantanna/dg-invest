  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Iniciar o Chrome em relação a localhost",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "SWA: Run web-app",
      "request": "launch",
      "type": "pwa-chrome",
      "url": "http://localhost:4280",
      "preLaunchTask": "swa: start web-app",
      "webRoot": "${workspaceFolder}/",
      "timeout": 30000
    }
  ]
}
